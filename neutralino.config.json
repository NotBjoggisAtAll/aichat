{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vue/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "aichat",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "vue/src/assets/logo.svg"
    }
  },
  "cli": {
    "binaryName": "aichat",
    "resourcesPath": "/vue/dist",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.13.0",
    "clientVersion": "3.11.0",
    "frontendLibrary": {
      "patchFile": "/vue/index.html",
      "devUrl": "http://localhost:5173"
  }
  }
}